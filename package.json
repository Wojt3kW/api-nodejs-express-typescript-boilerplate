{
  "name": "backend_app",
  "version": "0.0.0",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "init": "cp .env.example .env.development.local && cp .env.example .env.test.local && npm install -g dotenv-cli && npm install && npm audit",
    "start": "npm run build && cross-env NODE_ENV=production node dist/src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "test": "cross-env NODE_ENV=development jest --forceExit --detectOpenHandles",
    "test:debug": "node --nolazy --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --colors --verbose",
    "test:coverage": "cross-env NODE_ENV=development jest --coverage",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "prisma:init": "prisma init",
    "prisma:migrate": "dotenv -e .env.development.local -- npx prisma migrate dev --preview-feature",
    "prisma:generate": "prisma generate --schema src/prisma/schema.prisma",
    "prisma:seed": "dotenv -e .env.development.local -- npx prisma db seed",
    "prisma:reset": "dotenv -e .env.development.local -- npx prisma migrate reset",
    "npm-check-updates": "npx npm-check-updates",
    "install-dotenv-cli": "npm install dotenv-cli"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "ts-node src/prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.13.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.1",
    "envalid": "^8.0.0",
    "event-dispatch": "^0.4.1",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-ts-cache": "^4.4.0",
    "node-ts-cache-storage-memory": "^4.4.0",
    "reflect-metadata": "^0.2.2",
    "status-code-enum": "^1.0.0",
    "typedi": "^0.10.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.11.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-love": "^43.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "guid-typescript": "^1.0.9",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "node-config": "^0.0.2",
    "node-gyp": "^10.1.0",
    "nodemon": "^3.1.0",
    "npm-check-updates": "^16.14.18",
    "prettier": "^3.2.5",
    "prisma": "^5.13.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.2.2"
  }
}
